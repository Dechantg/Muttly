{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true // Enable JSX parsing
    },
    "requireConfigFile": false
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    // Your custom rules
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
